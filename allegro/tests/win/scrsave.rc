#include <windows.h>


STRINGTABLE DISCARDABLE
BEGIN
   1 "Allegro Screensaver"
END


ID_CONFIG_DLG DIALOG 6, 18, 160, 64
STYLE DS_MODALFRAME | WS_POPUP | WS_VISIBLE | WS_CAPTION | WS_SYSMENU
CAPTION "Allegro Screensaver Setup"
FONT 8, "MS Shell Dlg"
BEGIN
   DEFPUSHBUTTON "OK", IDOK, 100, 8, 50, 14
   PUSHBUTTON "Cancel", IDCANCEL, 100, 28, 50, 14
